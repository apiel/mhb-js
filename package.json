{
  "name": "mhb-js",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "DEBUG=zigbee-service* node src/index.js",
    "status": "systemctl status mhb-js.service",
