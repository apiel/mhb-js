  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "googleapis": "27",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "mock-require": "^3.0.3",
    "moment": "^2.23.0",
    "peer-ssdp": "^0.0.5",
    "readline": "^1.3.0",
    "serve-index": "^1.9.1",
    "suncalc": "^1.8.0",
    "zigbee2mqtt": "^1.14.1"
  }
}
